    "random-words": "^1.1.0",
    "readline": "^1.3.0",
    "timer-stopwatch": "git+https://github.com/Scoombe/timer-stopwatch.git",
    "wpmtest": "^1.3.8"
  },
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/node": "^13.1.8",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
