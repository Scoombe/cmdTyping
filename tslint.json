{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules"
    ],
    "jsRules": {},
    "rules": {
        "ban-types": false,
        "class-name": false,
        "max-line-length": false,
        "member-access": false,
        "no-console": false,
        "no-empty": false,
        "no-var-requires": false,
        "only-arrow-functions": false,
        "prefer-const": false,
        "quotemark": [true, "single", "avoid-escape"],
        "ter-indent": [true, 4, { "SwitchCase": 1 }],
        "trailing-comma": false
    },
    "rulesDirectory": []
}